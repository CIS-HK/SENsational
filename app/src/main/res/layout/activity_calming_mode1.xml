<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E6FAFF"
    tools:context=".Controller.Calming.CalmingMode1Activity">

    <Button
        android:id="@+id/backButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:onClick="backButton"
        android:text="@string/back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/pauseButton1"
        android:layout_width="101dp"
        android:layout_height="75dp"
        android:layout_marginBottom="24dp"
        android:onClick="pauseButton"
        android:text="@string/pause"
        android:textColor="#E6FAFF"
        android:textSize="45sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/cc1"
        android:layout_width="84dp"
        android:layout_height="116dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="181dp"
        android:layout_marginEnd="282dp"
        android:layout_marginBottom="434dp"
        android:rotation="-30"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cc1" />

    <ImageView
        android:id="@+id/candy1"
        android:layout_width="98dp"
        android:layout_height="84dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="171dp"
        android:rotation="-30"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.304"
        app:layout_constraintStart_toEndOf="@+id/candy2"
        app:layout_constraintTop_toBottomOf="@+id/candy3"
        app:layout_constraintVertical_bias="0.215"
        app:srcCompat="@drawable/candy1" />

    <ImageView
        android:id="@+id/candy2"
        android:layout_width="98dp"
        android:layout_height="84dp"
        android:layout_marginStart="457dp"
        android:layout_marginTop="758dp"
        android:layout_marginEnd="262dp"
        android:layout_marginBottom="252dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.566"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.572"
        app:srcCompat="@drawable/candy2" />

    <ImageView
        android:id="@+id/candy3"
        android:layout_width="97dp"
        android:layout_height="72dp"
        android:layout_marginStart="264dp"
        android:layout_marginTop="495dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="164dp"
        android:rotation="25"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.621"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.554"
        app:srcCompat="@drawable/candy3" />

    <ImageView
        android:id="@+id/cupcake"
        android:layout_width="190dp"
        android:layout_height="219dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="107dp"
        android:layout_marginBottom="141dp"
        android:rotation="-10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/candy2"
        app:layout_constraintHorizontal_bias="0.603"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/circle1"
        app:layout_constraintVertical_bias="0.258"
        app:srcCompat="@drawable/cupcake" />

    <ImageView
        android:id="@+id/lollipop"
        android:layout_width="158dp"
        android:layout_height="180dp"
        android:layout_marginStart="125dp"
        android:layout_marginTop="219dp"
        android:layout_marginEnd="318dp"
        android:layout_marginBottom="563dp"
        android:rotation="-30"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/lollipop" />

    <ImageView
        android:id="@+id/cc2"
        android:layout_width="91dp"
        android:layout_height="158dp"
        android:layout_marginStart="118dp"
        android:layout_marginTop="112dp"
        android:layout_marginEnd="202dp"
        android:layout_marginBottom="461dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cc2" />

    <ImageView
        android:id="@+id/circle1"
        android:layout_width="302dp"
        android:layout_height="298dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/circle" />

    <TextView
        android:id="@+id/breatheTextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="415dp"
        android:layout_marginBottom="467dp"
        android:text="@string/breathe_in"
        android:textColor="#000000"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/numberTextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="464dp"
        android:layout_marginBottom="406dp"
        android:text="@string/_1"
        android:textColor="#000000"
        android:textSize="45sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="237dp"
        android:layout_height="219dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="38dp"
        app:layout_constraintBottom_toTopOf="@+id/circle1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.864"
        app:layout_constraintStart_toEndOf="@+id/cc2"
        app:layout_constraintTop_toBottomOf="@+id/imageView29"
        app:layout_constraintVertical_bias="0.764"
        app:srcCompat="@drawable/cloud2" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="230dp"
        android:layout_height="216dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="167dp"
        android:layout_marginBottom="19dp"
        app:layout_constraintBottom_toTopOf="@+id/lollipop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.693"
        app:layout_constraintStart_toEndOf="@+id/lollipop"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.239"
        app:srcCompat="@drawable/cloud1" />

</androidx.constraintlayout.widget.ConstraintLayout>